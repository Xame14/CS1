    cout << "Enter the first number: ";
    cin >> num1;
    cout << "Enter the second number: ";
    cin >> num2;
    
    swapin(num1, num2);
    
    cout << "First number is "<<num1<<" second number is "<<num2<<"."<<endl;
    
    cout<<"Multiply time!"<<endl

    return 0;
}

void swapin(float& x, float& y)
{
   float swap = x; 
   x = y;
   y = swap;

}

